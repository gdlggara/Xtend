{
  "type": "object",
  "properties": {
    "isbossblank": {
      "type": "identifier",
      "selector": "h1",
      "control": "exists",
      "selectContains": "Home"
    },
    "routingviewerlink": {
      "type": "action",
      "selector": "a[href=\"/Xtend/BOSS/display/viewer\"]",
      "control": "action.link"
    }
  },
  "connect": [
    {
      "match": [
        "isbossblank"
      ],
      "action": "routingviewerlink"
    }
  ]
}
