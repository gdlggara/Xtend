{
  "type": "object",
  "properties": {
    "searchstring": {
      "type": "string"
    },
    "search": {
      "type": "action"
    },
    "routinglist": {
      "type": "select",
      "update": {
        "type": "none"
      },
      "selector": "#routings",
      "control": "select"
    },
    "serialnumberlist": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "partnumber": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 10,
            "control": "property.innerText"
          },
          "parentcodedescription": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 8,
            "control": "property.innerText"
          },
          "batchqty": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selectIndex": 12,
            "selector": "td",
            "control": "property.innerText"
          }
        }
      },
      "update": {
        "type": "none"
      },
      "selectorAll": ".k-grid-content > table tr[role=\"row\"]"
    },
    "isroutingviewer": {
      "type": "identifier",
      "selector": "h3",
      "control": "exists",
      "selectContains": "Routing"
    }
  },
  "comment": "filterandsort",
  "connect": [
    {
      "match": [
        "isroutingviewer"
      ],
      "screens": [
        "boss/bossroutingviewer"
      ]
    }
  ]
}
